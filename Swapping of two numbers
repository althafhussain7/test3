#include<stdio.h>
#include<conio.h>
void main(){
	int a,b,tempvar;
	printf("enter the value of a: ");
	scanf("%d",&a);
	printf("enter the value of b: ");
	scanf("%d",&b);
	tempvar=a;
	a=b;
	b=tempvar;
	printf("After swapping \n");
	printf("value of a is : %d\n",a);
	printf("value of b is : %d ",b);
getch();
} 
